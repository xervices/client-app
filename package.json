{
  "name": "expo-sample",
  "main": "expo-router/entry",
  "version": "0.1.8",
  "scripts": {
    "dev": "expo start",
    "generate": "orval",
    "generate:watch": "orval --watch",
    "generate:clean": "rm -rf src/api/generated && orval",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build -p android --profile preview",
    "build:ios": "eas build --platform ios",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:preview": "eas build -p ios --profile preview",
    "update": "eas update -p all --environment production --channel production",
    "update:android": "eas update -p android --environment production --channel production",
    "update:android:preview": "eas update -p android --environment preview --channel preview",
    "update:ios": "eas update -p ios --environment production --channel production,",
    "update:ios:preview": "eas update -p ios --environment preview --channel preview",
    "start": "expo start -c",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules"
  },
  "dependencies": {
    "@legendapp/list": "^2.0.16",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/accordion": "^1.2.0",
    "@rn-primitives/avatar": "^1.2.0",
    "@rn-primitives/checkbox": "^1.2.0",
    "@rn-primitives/dialog": "^1.2.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/select": "^1.2.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/tabs": "^1.2.0",
    "@tanstack/react-form": "^1.26.0",
    "@tanstack/react-query": "^5.90.12",
    "app-icon-badge": "^0.1.2",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^54.0.0",
    "expo-application": "~7.0.7",
    "expo-audio": "~1.1.0",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.9",
    "expo-contacts": "~15.0.11",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.9",
